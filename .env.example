# ============================================
# MadCamp02 Backend Environment Variables
# ============================================

# Database (PostgreSQL)
DB_HOST=localhost
DB_PORT=5432
DB_NAME=madcamp02-backend
DB_USERNAME=postgres
DB_PASSWORD=postgres

# Redis
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=

# JWT Secret (최소 32자 이상, 프로덕션에서는 반드시 변경!)
JWT_SECRET=dev-secret-key-for-local-development-minimum-32-characters-long

# Google OAuth2 (Google Cloud Console에서 발급)
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret

# Finnhub API (https://finnhub.io에서 발급)
FINNHUB_API_KEY=your-finnhub-api-key

# AI Server
AI_SERVER_URL=http://localhost:8000

# Server
SERVER_PORT=8080
SPRING_PROFILES_ACTIVE=dev
